<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/register_nav_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.cadastrousemobile.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/secondFragment" />
    </fragment>


    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.cadastrousemobile.SecondFragment"
        android:label="fragment_secont"
        tools:layout="@layout/fragment_second" >
        <argument android:name="nome"
            app:argType="string"
            app:nullable="false"/>
        <argument android:name="cpf"
            app:argType="string"
            app:nullable="false"/>
        <argument android:name="email"
            app:argType="string"
            app:nullable="false"/>
        <action
            android:id="@+id/action_secondFragment_to_thirdFragment"
            app:destination="@id/thirdFragment" />
    </fragment>

    <fragment
        android:id="@+id/thirdFragment"
        android:name="com.example.cadastrousemobile.ThirdFragment"
        android:label="fragment_third"
        tools:layout="@layout/fragment_third">

        <argument android:name="nome"
            app:argType="string"
            app:nullable="false"/>
        <argument android:name="cpf"
            app:argType="string"
            app:nullable="false"/>
        <argument android:name="email"
            app:argType="string"
            app:nullable="false"/>

        <argument android:name="cep"
            app:argType="string"
            app:nullable="false"/>
        <argument android:name="cidade"
            app:argType="string"
            app:nullable="false"/>
        <argument android:name="endereco"
            app:argType="string"
            app:nullable="false"/>
        <argument android:name="numero"
            app:argType="string"
            app:nullable="false"/>
        <argument android:name="complemento"
            app:argType="string"
            app:nullable="false"/>
    </fragment>

</navigation>